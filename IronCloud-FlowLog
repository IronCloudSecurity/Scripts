import boto3

def lambda_handler(event, context):
    logs_client = boto3.client('logs')
    
    # Retrieve the log group and log stream names from the event
    log_group_name = event['awslogs']['logGroup']
    log_stream_name = event['awslogs']['logStream']
    
    # Process the log events
    for log_event in event['awslogs']['logEvents']:
        # Access and process individual log event properties
        log_timestamp = log_event['timestamp']
        log_message = log_event['message']
        # Process the log event as desired
        print(f"Received log event at {log_timestamp}: {log_message}")

    return {
        'statusCode': 200,
        'body': 'Log processing completed.'
    }
